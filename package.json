    "morgan"         : "1.10.0",
    "should"         : "13.2.3",
    "should-http"    : "0.1.1"
  },
  "main": "index",
  "repository": {
    "type": "git",
    "url" : "https://github.com/dreamerslab/vodka.git"
  },
  "engines": [
    "node >= 14.0.0"
  ],
  "licenses": [
    {
