    "test",
    "testing",
    "functinal test",
    "restful client"
  ],
  "author": "dreamerslab <ben@dreamerslab.com>",
  "dependencies": {
    "request": "2.88.2"
  },
  "devDependencies": {
    "body-parser"    : "1.20.2",
    "express"        : "4.18.2",
